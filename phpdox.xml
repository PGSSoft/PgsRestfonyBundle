<phpdox xmlns='http://xml.phpdox.net/config'>
    <project name='pgs-soft/restfony-bundle' source='.' workdir='build/phpdox'>
        <collector publiconly='false'>
            <include mask='*.php' />
            <exclude mask='**Tests/**' />
            <exclude mask='**vendor/**' />
        </collector>

        <generator output='build'>
            <build engine='html' enabled='true' output='api'>
                <file extension='html' />
            </build>
        </generator>
    </project>
</phpdox>
